# Production Environment Configuration
# Set these in your production deployment system

NODE_ENV=production

# API Configuration
BASE_URL=https://api.prod.sportsbookengine.com/api
VERSION_2=/v2

# Client Configuration (REQUIRED - Set in production)
CLIENT_ID=

# MQTT Configuration
MQTT_URI=wss://emqx.sportsbookengine.com/mqtt
MQTT_USERNAME=
MQTT_PASSWORD=
MQTT_CLIENTID=cubebet_prod

# Performance & Security
DEBUG=false
API_LOGGING=false
GENERATE_SOURCEMAP=false

# Backend URLs for proxy
API_BACKEND_URL=https://api.prod.sportsbookengine.com
MQTT_BACKEND_URL=wss://emqx.sportsbookengine.com

# Domain for Traefik (if using)
DOMAIN=your-domain.com